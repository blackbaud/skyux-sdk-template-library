{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "appSettings": {
    "myLibrary": {
      "name": "My Library"
    }
  },
  "compileMode": "aot",
  "mode": "easy",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
